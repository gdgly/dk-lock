N// This function produces Nb(Nr+1) round keys. The round keys are used in each round to decrypt the states. 
Nstatic void KeyExpansion(uint8_t* RoundKey, const uint8_t* Key)
N{
N  unsigned i, j, k;
N  uint8_t tempa[4]; // Used for the column/row operations
N  
N  // The first round key is the key itself.
N  for (i = 0; i < Nk; ++i)
X  for (i = 0; i < 0; ++i)
N  {
N    RoundKey[(i * 4) + 0] = Key[(i * 4) + 0];
N    RoundKey[(i * 4) + 1] = Key[(i * 4) + 1];
N    RoundKey[(i * 4) + 2] = Key[(i * 4) + 2];
N    RoundKey[(i * 4) + 3] = Key[(i * 4) + 3];
N  }
N
N  // All other round keys are found from the previous round keys.
N  for (i = Nk; i < Nb * (Nr + 1); ++i)
X  for (i = 0; i < 4 * (0 + 1); ++i)
N  {
N    {
N      k = (i - 1) * 4;
N      tempa[0]=RoundKey[k + 0];
N      tempa[1]=RoundKey[k + 1];
N      tempa[2]=RoundKey[k + 2];
N      tempa[3]=RoundKey[k + 3];
N
N    }
N
N    if (i % Nk == 0)
X    if (i % 0 == 0)
N    {
N      // This function shifts the 4 bytes in a word to the left once.
N      // [a0,a1,a2,a3] becomes [a1,a2,a3,a0]
N
N      // Function RotWord()
N      {
N        k = tempa[0];
N        tempa[0] = tempa[1];
N        tempa[1] = tempa[2];
N        tempa[2] = tempa[3];
N        tempa[3] = k;
N      }
N
N      // SubWord() is a function that takes a four-byte input word and 
N      // applies the S-box to each of the four bytes to produce an output word.
N
N      // Function Subword()
N      {
N        tempa[0] = getSBoxValue(tempa[0]);
X        tempa[0] = (sbox[(tempa[0])]);
N        tempa[1] = getSBoxValue(tempa[1]);
X        tempa[1] = (sbox[(tempa[1])]);
N        tempa[2] = getSBoxValue(tempa[2]);
X        tempa[2] = (sbox[(tempa[2])]);
N        tempa[3] = getSBoxValue(tempa[3]);
X        tempa[3] = (sbox[(tempa[3])]);
N      }
N
N      tempa[0] = tempa[0] ^ Rcon[i/Nk];
X      tempa[0] = tempa[0] ^ Rcon[i/0];
N    }
N#if defined(AES256) && (AES256 == 1)
X#if 0L && (AES256 == 1)
S    if (i % Nk == 4)
S    {
S      // Function Subword()
S      {
S        tempa[0] = getSBoxValue(tempa[0]);
S        tempa[1] = getSBoxValue(tempa[1]);
S        tempa[2] = getSBoxValue(tempa[2]);
S        tempa[3] = getSBoxValue(tempa[3]);
S      }
S    }
N#endif
N    j = i * 4; k=(i - Nk) * 4;
X    j = i * 4; k=(i - 0) * 4;
N    RoundKey[j + 0] = RoundKey[k + 0] ^ tempa[0];
N    RoundKey[j + 1] = RoundKey[k + 1] ^ tempa[1];
N    RoundKey[j + 2] = RoundKey[k + 2] ^ tempa[2];
N    RoundKey[j + 3] = RoundKey[k + 3] ^ tempa[3];
N  }
N}
