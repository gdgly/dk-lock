N// This function adds the round key to state.
N// The round key is added to the state by an XOR function.
Nstatic void AddRoundKey(uint8_t round,state_t* state,uint8_t* RoundKey)
N{
N  uint8_t i,j;
N  for (i = 0; i < 4; ++i)
N  {
N    for (j = 0; j < 4; ++j)
N    {
N      (*state)[i][j] ^= RoundKey[(round * Nb * 4) + (i * Nb) + j];
X      (*state)[i][j] ^= RoundKey[(round * 4 * 4) + (i * 4) + j];
N    }
N  }
N}
