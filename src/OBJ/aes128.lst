Nstatic void XorWithIv(uint8_t* buf, uint8_t* Iv)
N{
N  uint8_t i;
N  for (i = 0; i < AES_BLOCKLEN; ++i) // The block in AES is always 128bit no matter the key size
X  for (i = 0; i < 16; ++i) 
N  {
N    buf[i] ^= Iv[i];
