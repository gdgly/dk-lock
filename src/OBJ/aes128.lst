N    if (bi == AES_BLOCKLEN) /* we need to regen xor compliment in buffer */
X    if (bi == 16)  
N    {
N      
N      memcpy(buffer, ctx->Iv, AES_BLOCKLEN);
X      memcpy(buffer, ctx->Iv, 16);
