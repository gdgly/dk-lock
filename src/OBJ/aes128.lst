Nstatic uint8_t xtime(uint8_t x)
N{
N  return ((x<<1) ^ (((x>>7) & 1) * 0x1b));
N}
N
N// MixColumns function mixes the columns of the state matrix
Nstatic void MixColumns(state_t* state)
N{
N  uint8_t i;
N  uint8_t Tmp, Tm, t;
N  for (i = 0; i < 4; ++i)
N  {  
N    t   = (*state)[i][0];
N    Tmp = (*state)[i][0] ^ (*state)[i][1] ^ (*state)[i][2] ^ (*state)[i][3] ;
N    Tm  = (*state)[i][0] ^ (*state)[i][1] ; Tm = xtime(Tm);  (*state)[i][0] ^= Tm ^ Tmp ;
N    Tm  = (*state)[i][1] ^ (*state)[i][2] ; Tm = xtime(Tm);  (*state)[i][1] ^= Tm ^ Tmp ;
N    Tm  = (*state)[i][2] ^ (*state)[i][3] ; Tm = xtime(Tm);  (*state)[i][2] ^= Tm ^ Tmp ;
N    Tm  = (*state)[i][3] ^ t ;              Tm = xtime(Tm);  (*state)[i][3] ^= Tm ^ Tmp ;
N  }
N}
